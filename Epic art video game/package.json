{
  "name": "epic-art-video-game",
  "version": "0.3.0",
  "description": "Sweet Adventure",
  "developers": {
    "name": "Witchdoctor"
  },
  "main": "app.js",
  "author": {
    "name": "Babah Gee",
    "url": "https://github.com/babahgee"
  },
  "scripts": {
    "install": "npm install express electron-is-dev electron socket.io socket.io-client colors cheerio",
    "start": "electron .",
    "start-novsync": "electron . --disable-frame-rate-limit",
