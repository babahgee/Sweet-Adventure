  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "colors": "^1.4.0",
    "electron": "^15.1.2",
    "electron-is-dev": "^2.0.0",
    "express": "^4.17.1",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0"
  },
  "repository": {
    "url": "https://github.com/babahgee/Sweet-Adventure"
  }
}
